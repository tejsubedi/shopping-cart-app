{{# each products}}
<div class="row">
  {{# each this}}
  <div class="col-sm-4 col-md-3">
    <div class="card" style="width: 18rem;">
      <div class="caption">
        <h3>{{this.title}}</h5>
          <p class="description">{{this.description}}</p>
          <div class="clearfix">
            <div class="price pull-left btn-dark">$ {{this.price}}
              <a href="/add-to-cart/{{this._id}}" class="btn btn-success pull-right" role="button">Add to shopping cart</a>
            </div>
          </div>
      </div>
    </div>
    {{/each}}
  </div>
  {{/each}}